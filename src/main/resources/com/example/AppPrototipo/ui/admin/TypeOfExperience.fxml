<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.AppPrototipo.ui.admin.TypeOfExperienceController"
            prefHeight="400.0" prefWidth="600.0">
    <top>
    </top>
    <bottom>
        <HBox alignment="CENTER" prefHeight="54.0" prefWidth="472.0" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="agregarBtn" mnemonicParsing="false" onAction="#agregarExperienceType" text="Agregar" />
                <Button fx:id="cancelarBtn" mnemonicParsing="false" onAction="#close" text="Cancelar" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
    </bottom>
    <center>
        <AnchorPane BorderPane.alignment="CENTER">
            <Label layoutX="54.0" layoutY="30.0" text="Nombre" />
            <TextField fx:id="nameInput" layoutX="169.0" layoutY="25.0" prefHeight="26.0" prefWidth="245.0" />
            <Label layoutX="55.0" layoutY="290.0" text="Intereses" />
            <VBox fx:id="interestVBox" layoutX="168.0" layoutY="290.0" prefHeight="25.0" prefWidth="247.0" spacing="10.0" />
        </AnchorPane>
    </center>
</BorderPane>
