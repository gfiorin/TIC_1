<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.AppPrototipo.ui.admin.TypeOfExperienceController">
    <top>
        <AnchorPane BorderPane.alignment="CENTER">
            <Label layoutX="30.0" layoutY="30.0" text="Nombre" />
            <TextField fx:id="nameInput" layoutX="100.0" layoutY="25.0" prefHeight="26.0" prefWidth="245.0" />
            <Label layoutX="30.0" layoutY="80.0" text="Intereses" />
            <ComboBox fx:id="interestInput" layoutX="100.0" layoutY="80.0" prefHeight="26.0" prefWidth="246.0" />
        </AnchorPane>
    </top>
    <bottom>
        <HBox alignment="CENTER" prefHeight="54.0" prefWidth="472.0" spacing="10.0" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <Button fx:id="agregarBtn" mnemonicParsing="false" onAction="#agregarExperienceType" text="Agregar" />
            <Button fx:id="cancelarBtn" mnemonicParsing="false" onAction="#close" text="Cancelar" />
        </HBox>
    </bottom>
</BorderPane>
